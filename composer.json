{
    "name": "codeacious/oauth2-provider",
    "description": "A simple and clean Zend Framework 2 wrapper for the oauth2-server-php library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Glenn Schmidt",
            "email": "glenn@codeacious.com",
            "homepage": "https://codeacious.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "ext-json": "*",
        "bshaffer/oauth2-server-php": "~1.0",
        "laminas/laminas-http": "^2.11",
        "laminas/laminas-filter": "^2.9",
        "laminas/laminas-servicemanager": "^3.4",
        "laminas/laminas-authentication": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "suggest": {
        "laminas-api-tools/api-tools-mvc-auth": "Use with the included adapter to authenticate requests using access tokens"
    },
    "autoload": {
        "psr-4": {
            "Codeacious\\OAuth2Provider\\": "src/",
            "CodeaciousTest\\OAuth2Provider\\": "test/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-trunk": "1.0.x-dev"
        }
    }
}
